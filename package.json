{
  "name": "nfc-east-react",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build:data": "node scripts/build-data.mjs",
    "build:data:watch": "node scripts/build-data.mjs --watch",
    "fetch:logos": "node scripts/fetch-logos.mjs && npm run optimize:logos && npm run build:data",
    "optimize:logos": "svgo -f public/logos --config=svgo.config.mjs",
    "deploy": "bash scripts/deploy.sh",
    "predev": "npm run build:data",
    "prebuild": "npm run build:data",
    "dev": "npm run build:data:watch & vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "js-yaml": "4.1.0"
  },
  "devDependencies": {
    "svgo": "3.3.2",
    "vite": "5.4.11"
  }
}
